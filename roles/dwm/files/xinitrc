#!/bin/sh

#
# ~/.xinitrc
#
# Executed by startx (run your window manager from here)
#
exec dwm &
dwmstatus 2>&1 >/dev/null &
setxkbmap de nodeadkeys -option caps:escape &
xrandr --output eDP1 --mode 1920x1080 --output HDMI1 --primary --mode 1920x1080 --right-of eDP1 --output DP1 --mode 1920x1080 --right-of HDMI1 &
feh --bg-scale ~/Seafile/My\ Library/wallpaper/hollowknight.png &
xclip &
dunst
